				      <input type="text" class="form-control" id="street2" placeholder="">
				      <span id="helpBlock" class="help-block error-block">&nbsp;</span>
				  </div>
				  <div class="form-group">
				    <label for="city" class="control-label">City:</label>
				      <input type="text" class="form-control" id="city" placeholder="" required>
				      <span id="helpBlock" class="help-block error-blok">&nbsp;</span>
				  </div>
				  <div class="form-group col-xs-8 no-padding-left">
				        <label for="state" class="control-label">State</label>
				        <select class="form-control" id="state" required>
						<option value="">Select One</option>
						<option value="other">Other</option>	

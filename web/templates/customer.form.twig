<form class="createCustomerForm">
	<div class="tabbable">
		<ul class="nav nav-tabs modal-tab" role="tablist" id="customer-tab" >
  			<li role="presentation" class="active">
  				<a href="#personal-info" aria-controls="home" role="tab" data-toggle="tab">Home</a></li>
  			<li role="presentation">
  				<a href="#address-info" aria-controls="profile" role="tab" data-toggle="tab">Profile</a></li>
		</ul>
		<div class="tab-content">
			<div class="tab-pane active" id="personal-info">
			  <div class="form-group">
			    <label for="firstName" class="control-label">First Name:</label>
			      <input type="text" class="form-control" id="firstName" placeholder="" required>
			      <span id="helpBlock" class="help-block error-block">&nbsp;</span>
			  </div>
			  <div class="form-group">
			    <label for="lastName" class="control-label">Last Name:</label>
			      <input type="text" class="form-control" id="lastName" placeholder="" required>
			      <span id="helpBlock" class="help-block error-block">&nbsp;</span>
			  </div>
			  <div class="form-group">
			    <label for="email" class="control-label">Email:</label>
			      <input type="email" class="form-control" id="email" placeholder="" required>
			      <span id="helpBlock" class="help-block error-blok">&nbsp;</span>
			  </div>
			  <div class="form-group">
			    <label for="phoneNumber" class="control-label">Phone Number:</label>
			      <input type="text" class="form-control" id="phoneNumber" placeholder="" required>
			      <span id="helpBlock" class="help-block error-block">&nbsp;</span>
			  </div>
			  <div class="checkbox">
			    <label>
			      <input type="checkbox" id="taxable" checked> Taxable
			    </label>
			  </div> 
			  
			</div>
			<div class="tab-pane" id="address-info">
				<div class="form-group">
				    <label for="street" class="control-label">Street Address:</label>
